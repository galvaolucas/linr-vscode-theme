{
  "name": "Linr Theme",
  "colors": {
		"activityBar.background": "#242038",
    "activityBar.border": "#8D86C9",
    "activityBar.activeFocusBorder": "#8D86C9",
    "activityBar.foreground": "#4EC9B0",
		"badge.background": "#2092715b",
		"sideBar.background": "#2a233f89",
    "editorCursor.foreground": "#bd93f9",
    "statusBar.background": "#8D86C9",
    "statusBar.debuggingBackground": "#725AC1",
    "statusBarItem.remoteBackground": "#8D86C9",
    "breadcrumb.activeSelectionForeground": "#8D86C9",
    "button.background": "#4EC9B0",
    "commandCenter.activeBackground": "#09715c",
    "list.activeSelectionBackground": "#031B29",
    "focusBorder": "#031B29",
    "list.hoverBackground": "#053048",
    "list.errorForeground": "#ff5454",
    "sideBar.foreground": "#E8E0D1",
    "editor.lineHighlightBackground": "#242038",
    "editor.selectionBackground": "#053048",
    "editor.findMatchBackground": "#053048",
    "editor.selectionHighlightBackground": "#2092715b",
    "editor.findMatchHighlightBackground": "#2092715b",
    "foreground": "#F9F7F3",
    "editor.foreground": "#CAC4CE",
    "editor.background": "#242038",
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "variable.language"
      ],
      "settings": {
        "foreground": "#b0fcc1"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#8ddcde"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FEFEFE"
      }
    },
    {
      "scope": [
        "entity.name.function",
      ],
      "settings": {
        "foreground": "#B4D3FA"
      }
    },
		{
      "scope": [
        "entity.name.tag",
        "entity.name.section",
        "entity.name.attribute",
        "entity.name.attribute-name"
      ],
      "settings": {
        "foreground": "#725AC1"
      }
    },
    {
      "scope": [
        "constant",
        "constant.language",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#D0A9EA"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.other",
        "string.unquoted",
      ],
      "settings": {
        "foreground": "#F7ECE1"
      }
    },
		{
			"scope": [
				"support.class",
				"support.type",
				"support.class",
				"support.interface",
        "support.function",
        "support.variable"
			],
			"settings": {
				"foreground": "#EDDEA4"
			}
		},
    {
			"name": "Types declaration and references",
			"scope": [
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
    {
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
    {
			"name": "Keywords",
			"scope": [
        "keyword"
			],
			"settings": {
				"foreground": "#5BC2A0"
			}
		},
    {
			"name": "General",
			"scope": [
        "markup",
        "emphasis",
        "storage",
        "punctuation.terminator",
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
  ]
}